statement:printf(<argument-expression-list>)\;=format-out($1)\;@end
statement:fprintf(<argument-expression-list>)\;=format($1)\;@end
statement:sprintf(<postfix-expression>,\W<argument-expression-list>)\;=\
	$1 \:\= format-to-string($2)\;@end

struct <I><space>\{<fields>\}<space>*\;=\
	@set{sclass;\<$1\>}\
	define class ${sclass} ( \<object\> )$2\N$3\Nend class ${sclass}\;$4\
	\N@sdefs{*}\N
