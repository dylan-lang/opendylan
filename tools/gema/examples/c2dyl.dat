! required space
ReqSpace:\/\/*\n= $0
ReqSpace:\/\*<comment>\*\/= $0 ;
ReqSpace:\N\W\#*\n=$0
ReqSpace:\s=\s
ReqSpace:<S>=$1
ReqSpace:=@terminate

code:\"<string>\"=$0
string:\\?=\\?
code:\'<char>\'=$0
char:\\?=\\?@end;?=?@end

\n\L\W\P\n=\n
code:\n\L\W\P\n=\n

code:\=\==\S\=\S
code:\==\S\:\=\S
code:\!\==\S\~\=\S
code:\<\==\S\<\=\S
code:\>\==\S\>\=\S
code:\<=\S\<\S
code:\>=\S\>\S
code:-\>=.
code:\!=\S\~\S
code:\~\W(<code>)=\Ilognot($1)
code:\~\W<I>=\Ilognot(@rid{$1})
code:\~=\Ilognot\I\/\* \?\?\? \*\/

! quick pass-through of white space before trying to match <type>
\s=\s
\n=\n
\t=\t

